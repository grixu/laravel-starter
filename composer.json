{
    "name": "grixu/laravel-starter",
    "type": "project",
    "description": "Domain-oriented template for Laravel Framework.",
    "keywords": ["framework", "laravel", "template"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "^3.191",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "laravel-lang/lang": "~10.0",
        "laravel/framework": "^8.77",
        "laravel/tinker": "^2.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9",
        "friendsofphp/php-cs-fixer": "^3.4",
        "laravel/sail": "^1.12",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^1.0.1",
        "nunomaduro/phpinsights": "^2.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.27",
        "spatie/x-ray": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Domain\\": "src/Domain/",
            "Support\\": "src/Support/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "insights": "php artisan insights",
        "cs": "vendor/bin/php-cs-fixer fix src",
        "stan": "vendor/bin/phpstan analyse",
        "xray": "vendor/bin/x-ray src"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
